<!-- 页面访问信息区域 -->
<div id="page-access-info" class="bg-green-50 border border-green-200 rounded-lg p-4 hidden">
  <div class="flex items-center space-x-2 mb-3">
    <span class="material-symbols-outlined text-green-600 text-lg">language</span>
    <span class="font-medium text-sm text-green-800">登录页面已发布</span>
  </div>
  <div class="bg-white rounded p-3 border">
    <div class="text-xs text-gray-700 mb-2">局域网访问地址:</div>
    <div class="flex items-center space-x-2">
      <code id="lan-access-url" class="text-sm font-mono bg-gray-100 px-2 py-1 rounded flex-1"></code>
      <button id="copy-url-btn" class="px-2 py-1 bg-blue-600 hover:bg-blue-700 text-white text-xs rounded transition-colors">复制</button>
    </div>
  </div>
</div>
